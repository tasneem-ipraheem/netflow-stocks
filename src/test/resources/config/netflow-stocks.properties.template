spring.main.show_banner=false
spring.datasource.url=jdbc:mysql://<HOST>:<PORT>/stocks
spring.datasource.username=<USER>
spring.datasource.password=<PASSWORD>
spring.datasource.driver-class-name=com.mysql.jdbc.Driver

management.port=<MANAGEMENT_PORT>
management.context-path=/manage
server.port=<HTTP_PORT>

netflow.users.admin.name=<ADMIN_USER>
netflow.users.admin.password=<ADMIN_PASSWORD>
netflow.stocks.scheduler.cron=<CRON_EXPRESSION>
netflow.stocks.lookup.retry.timeout.milliseconds=500
netflow.stocks.lookup.retry.times=10
netflow.stokcs.security.enable=true

logging.file=netflow-stocks.log
logging.level.com.netflow.stocks=DEBUG